{
  "name": "purrooser",
  "description": "Web browser with Python",
  "scripts": {
    "setup:unix": "python3 -m venv venv",
    "run:unix": "source venv/bin/activate; cd core; python -m main; cd ..; deactivate;",
    "bootstrap:unix": "#!/bin/bash; source venv/bin/activate && spip install -r requirements.txt;",

    "run:win": "cd core; python -m main; cd ..;",
    "bootstrap:win": "pip install -r requirements.txt;",

    "compile:ts": "tsc",
    "compile:scss": "sass --update ui:ui",
    "compile": "npm run compile:ts && npm run compile:scss"
  },
  "author": "Thoq",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.5.31",
    "sass": "^1.79.4",
    "scss": "^0.2.4",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  }
}